{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "customManagers": [
      {
        "customType": "regex",
        "fileMatch": [
          "(^|/)Dockerfile$"
        ],
        "matchStrings": [
          "FROM (?<packageName>.*):.*-jre-(?<currentValue>.*)"
        ],
        "extractVersionTemplate": "^.*-jre-(?<version>.*)$",
        "depNameTemplate": "eclipse-temurin-variant",
        "versioningTemplate": "ubuntu",
        "datasourceTemplate": "docker"
      }
    ]
}
